  "author": {
    "name": "Blake Regalia",
    "email": "blake.regalia@gmail.com"
  },
  "type": "module",
  "main": "dist/cjs/main.js",
  "module": "dist/mjs/main.js",
  "types": "dist/mjs/main.d.ts",
  "exports": {
    "require": "./dist/cjs/main.js",
    "import": "./dist/mjs/main.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:mjs": "yarn tsc -p tsconfig.mjs.json",
    "build:cjs": "yarn tsc -p tsconfig.cjs.json",
    "build": "yarn build:mjs && yarn build:cjs",
    "bundle": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "test": "yarn build && bun test ./test/*.ts"
  },
  "devDependencies": {
    "@blake.regalia/eslint-config-elite": "^0.4.4",
    "@blake.regalia/tsconfig": "^0.2.0",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.11.30",
    "@types/web": "^0.0.142",
    "@typescript-eslint/parser": "^7.3.1",
    "bun": "^1.0.35",
    "bun-types": "^1.0.35",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "rollup": "^4.13.0",
    "ts-node": "^10.9.2",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^5.4.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
